{
  "name": "Karandeep Chrome Extension",
  "description": "A color picker extension",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/icon_16.png",
    "32": "/icon_32.png",
    "48": "/icon_48.png"
  }
}
